}

.category-items .basic-category .name {
  color: #ffffff;
  margin-right: 10rpx;
}

.category-items .basic-category.active .name {
  color: #ffe200;
}

.category-items .basic-category .mark {
  font-size: 13rpx;
  color: #ffe200;
  font-weight: 200;
}

.category-items .basic-category image {
  width: 100%;
  height: 100%;
  position: absolute;
  left: 0;
  right: 0;
  z-index: 0;
}

.category-items .basic-category:nth-child(2n) {
  right: 0;
}

.category-items .basic-category:nth-child(2n + 1) {
  left: 0;
}

.position-items {
  position: relative;
  height: 100rpx;
}

.position-items .position {
  position: absolute;
  width: 81.5rpx;
  height: 54rpx;
  background-image: url('data:image/png;base64,');
  background-size: 100%;
  background-repeat: no-repeat;
}

.position-items .position.out::after {
  content: 'å¤–';
  position: absolute;
  right: 10rpx;
  bottom: -4rpx;
  font-size: 18rpx;
  color: #ffe200;
  transform: scaleX(1.6);
}

.position-items .position image {
  position: absolute;
  width: 81.5rpx;
  height: 54rpx;
  z-index: 0;
}

.position-items .position .name {
  position: absolute;
  top: 50%;
  font-size: 28rpx;
  color: #ffffff;
  left: 50%;
  transform: translate(-50%, -50%);
}
.position-items .position.active .name {
  color: #ffe200;
}

