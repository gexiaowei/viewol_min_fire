@import '../../weui.wxss';
.exhibitors-container {
  display: flex;
  flex-direction: column;
  background: #9abffc;
}
.page__hd {
  padding: 30rpx 0;
}

.category-items {
  position: relative;
  clear: both;
  height: 190rpx;
}

.category-items .basic-category {
  position: absolute;
  height: 54rpx;
  display: flex;
  align-items: center;
  box-sizing: border-box;
  background-size: 100%;
  background-repeat: no-repeat;
}

.category-items .basic-category:nth-child(odd) {
  justify-content: flex-start;
  padding-left: 30rpx;
}

.category-items .basic-category:nth-child(even) {
  justify-content: flex-end;
  padding-right: 30rpx;
}

.category-items .basic-category:nth-child(1) {
  width: 288.5rpx;
  top: 0;
  background-image: url('data:image/png;base64,');
}

