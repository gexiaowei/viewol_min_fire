.search-container {
  position: absolute;
  left: 368rpx;
  top: 0;
  width: 268rpx;
}

.search-container .search-background {
  width: 100%;
  height: 54rpx;
  position: absolute;
  top: 0;
  z-index: 0;
}

.search-container .search-icon {
  width: 23rpx;
  height: 23rpx;
  position: absolute;
  top: 16rpx;
  left: 32rpx;
}

.search-container input {
  font-size: 21rpx;
  width: 140rpx;
  left: 66rpx;
  position: absolute;
  top: 5rpx;
  color: #a0c9fd;
}

.exhibitors-content-container {
  padding: 0 30rpx 21rpx;
}

.exhibitors .item {
  margin-top: 20rpx;
  background: #cee3fd;
  border: 2rpx solid rgba(215, 35, 22, 0.3);
  position: relative;
  box-shadow: none;
}

.exhibitors .item::after {
  content: '';
  position: absolute;
  left: 0;
  bottom: 0;
  border-style: solid;
  border-width: 30rpx 0 0 15rpx;
  border-color: transparent transparent transparent #d72316;
}

.exhibitors .item .logo-container {
  background: white;
  justify-content: center;
  align-items: center;
  border-radius: 10rpx;
  padding: 5rpx;
}

.exhibitors .item .title {
  font-size: 30rpx;
  color: rgba(51, 51, 51, 1);
  align-items: center;
  color: #0251b6;
  border-bottom: 2rpx solid #0251b6;
  padding: 3rpx 0;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.exhibitors .item .title .name {
  flex: 1;
  word-break: break-all;
  word-wrap: break-word;
}

.exhibitors .item .title .mark {
  height: 30rpx;
  margin-left: 10rpx;
  font-size: 20rpx;
  border: 2rpx solid rgba(215, 35, 22, 0.3);
  color: #d72316;
  padding: 0 6rpx;
  border-radius: 6rpx;
}

.exhibitors .item .desc {
  font-size: 24rpx;
  color: rgba(128, 128, 128, 1);
  line-height: 38rpx;
  padding: 3rpx 0;
  color: #0251b6;
}
.exhibitors {
  background: transparent;
}

.drawer_screen {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  opacity: 0.2;
  overflow: hidden;
  z-index: 1000;
  color: #fff;
}

.drawer_attr_box {
  width: 100%;
  overflow: hidden;
  position: fixed;
  bottom: 0;
  left: 0;
  z-index: 2000;
  background: #fff;
}

.drawer_attr_content {
  height: 600rpx;
  overflow-y: scroll;
}

.drawer_desc {
  padding: 20rpx;
  font-size: 26rpx;
  border-bottom: 1rpx solid #d9e6f2;
}
